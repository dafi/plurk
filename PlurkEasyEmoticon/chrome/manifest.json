{
  "name": "Plurk Emoticon Toolbar",
  "version": "1.2.3",
  "description": "__MSG_extDescription__",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://www.plurk.com/*"],
      "css": ["css/plurkEasyEmoticon.css"],
      "js": ["js/plurkEasyEmoticon.js"],
      "run_at": "document_end"
    }
   ],
  "permissions": [
    "tabs",
    "http://www.plurk.com/*"
  ],
   "icons": {
      "48": "images/icon48.png",
      "32": "images/icon32.png",
      "16": "images/icon16.png"
   },
  "default_locale": "en"
}
